{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js", 
      "use": "@vercel/node"
    },
    {
      "src": "static/*",
      "use": "@vercel/static" 
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"  
    }
  ]
}